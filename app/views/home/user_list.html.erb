<% if user_signed_in? %>
  <h1>Nos membres très privés</h1>

  <table>
    <thead>
      <tr>
        <th>Nom</th>
        <th>| Prénom</th>
        <th>| E-mail</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @user.each do |user| %>
        <tr>
          <td><%= user.nom %></td>
          <td>| &nbsp;  <%= user.prenom %></td>
          <td>| &nbsp; <%= user.email %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <% else %>

       <h1> On vous a dit de vous connecter XD ! </h1>

  <% end %>

